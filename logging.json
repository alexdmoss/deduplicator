{
  "version": 1,
  "disable_existing_loggers": true,
  "formatters": {
    "json_formatter": {
      "()": "pythonjsonlogger.jsonlogger.JsonFormatter",
      "format": "%(timestamp)s %(levelname)s %(name)s %(message)s",
      "timestamp": true,
      "rename_fields": {
        "levelname": "severity",
        "msg": "message"
      }
    }
  },
  "handlers": {
    "json": {
      "formatter": "json_formatter",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "deduplicator.images": {
      "handlers": [
        "json"
      ],
      "level": "INFO",
      "propagate": true
    },
    "deduplicator.files": {
      "handlers": [
        "json"
      ],
      "level": "INFO",
      "propagate": true
    },
    "deduplicator.results": {
      "handlers": [
        "json"
      ],
      "level": "INFO",
      "propagate": true
    },
    "deduplicator.utils": {
      "handlers": [
        "json"
      ],
      "level": "INFO",
      "propagate": true
    },
    "root": {
      "level": "INFO",
      "propagate": true,
      "handlers": [
        "json"
      ]
    }
  }
}
